pkgname=ezarcher-adjustment-modeling-nautilus
pkgver=0.1
pkgrel=1
pkgdesc="Ezarcher Adjustment / Modeling / Nautilus"
url="https://github.com/samwhelp/ezarcher-adjustment/tree/main/project/ezarcher-adjustment-system/ezarcher-adjustment-packaging/pack/core/tool/ezarcher-adjustment-modeling-nautilus"
arch=('any')
license=('MIT')
makedepends=()
depends=()
conflicts=()
provides=("${pkgname}")
options=(!strip !emptydirs)


prepare () {

	cp -af ../asset/. "${srcdir}"

}


package () {

	##
	## Main Config
	##

	cp -af "${srcdir}/overlay/." "${pkgdir}"

}
